        {
          "id": "d6f096d86444_33",
          "href": null,
          "name": "59a9",
          "text": "(a) knowledgeable about the global situation;",
          "metadata": null,
          "mixtapeMetadata": null,
          "hasDropCap": null,
          "dropCapImage": null,
          "iframe": null,
          "layout": null,
          "markups": [
