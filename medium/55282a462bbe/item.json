        {
          "id": "15a83f95178_4",
          "href": null,
          "name": "bed6",
          "text": "— Johann Wolfgang von Goethe, 1781",
          "metadata": null,
          "mixtapeMetadata": null,
          "hasDropCap": null,
          "dropCapImage": null,
          "iframe": null,
          "layout": null,
          "markups": [],
