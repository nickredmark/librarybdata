        {
          "id": "adcd1d681aa8_6",
          "href": null,
          "name": "6321",
          "text": "I had the good fortune to sit down with change strategist and culture designer Joe Brewer a few months ago and ask him some challenging questions to help understand and articulate the context of the complex and often confusing times we are living in.",
          "metadata": null,
          "mixtapeMetadata": null,
          "hasDropCap": null,
          "dropCapImage": null,
          "iframe": null,
          "layout": null,
          "markups": [
            {
              "end": 89,
              "href": "http://moneyandlifemovie.us2.list-manage1.com/track/click?u=22a3bc102bd637e56b0073878&id=1ecd720313&e=c56612d6c4",
              "rel": "nofollow noopener",
              "start": 79,
              "title": "",
              "anchorType": "LINK",
              "linkMetadata": null,
              "type": "A",
              "name": null,
              "creatorIds": null
            },
            {
              "end": 250,
              "href": null,
              "rel": null,
              "start": 0,
              "title": null,
              "anchorType": null,
