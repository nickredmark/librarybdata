            }
          ],
          "type": "P"
        },
        {
          "id": "3eb04489fdf0_2",
          "href": null,
          "name": "70a5",
          "text": "The Sharing Economy (Source)",
          "metadata": {
            "id": "1*j3imMYZ4MmooSzH_soaZpA.jpeg",
            "alt": null
          },
          "mixtapeMetadata": null,
          "hasDropCap": null,
          "dropCapImage": null,
          "iframe": null,
          "layout": "INSET_CENTER",
          "markups": [
            {
              "end": 27,
              "href": "https://static1.squarespace.com/static/54c8eb88e4b023b04977685a/t/578df55d03596ed89d1a3ff3/1468921187457/?format=750w",
