              "start": 0,
              "title": null,
              "anchorType": null,
              "linkMetadata": null,
              "type": "STRONG",
              "name": null,
              "creatorIds": null
            }
          ],
          "type": "P"
        },
        {
          "id": "1ffe021c4718_22",
          "href": null,
          "name": "fd8a",
          "text": "What can we learn from trusting our subjective experience more deeply?",
          "metadata": null,
          "mixtapeMetadata": null,
          "hasDropCap": null,
          "dropCapImage": null,
          "iframe": null,
          "layout": null,
          "markups": [
            {
              "end": 70,
              "href": null,
              "rel": null,
              "start": 0,
              "title": null,
              "anchorType": null,
              "linkMetadata": null,
              "type": "STRONG",
              "name": null,
              "creatorIds": null
            }
          ],
          "type": "P"
        },
        {
          "id": "1ffe021c4718_23",
          "href": null,
          "name": "a8f1",
          "text": "What can we learn from taking the ‘long view’?",
          "metadata": null,
          "mixtapeMetadata": null,
          "hasDropCap": null,
          "dropCapImage": null,
          "iframe": null,
          "layout": null,
          "markups": [
            {
              "end": 46,
