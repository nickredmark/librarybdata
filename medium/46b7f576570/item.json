              "title": null,
              "anchorType": null,
              "linkMetadata": null,
              "type": "STRONG",
              "name": null,
              "creatorIds": null
            }
          ],
          "type": "P"
        },
        {
          "id": "1ffe021c4718_24",
          "href": null,
          "name": "0e35",
          "text": "What would insightful action look like?",
          "metadata": null,
          "mixtapeMetadata": null,
          "hasDropCap": null,
          "dropCapImage": null,
          "iframe": null,
          "layout": null,
          "markups": [
            {
              "end": 39,
              "href": null,
