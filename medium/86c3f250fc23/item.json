              "creatorIds": null
            },
            {
              "end": 168,
              "href": "http://d20eq91zdmkqd.cloudfront.net/assets/images/book/large/9781/8836/9781883647186.jpg",
              "rel": "noopener",
              "start": 163,
              "title": "",
              "anchorType": "LINK",
